{
"manifest_version": 2,
    "name": "Opposite Day",
    "description": "See what opposite day is like",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}